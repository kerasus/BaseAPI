{
  "name": "api-basekit",
  "version": "1.0.2",
  "description": "Abstract base class for Axios-powered HTTP services with pagination helpers.",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "author": "a.esmaeeli",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kerasus/BaseAPI.git"
  },
  "bugs": {
    "url": "https://github.com/kerasus/BaseAPI/issues"
  },
  "homepage": "https://github.com/kerasus/BaseAPI#readme",
  "keywords": [
    "axios",
    "typescript",
    "api",
    "pagination",
    "abstract-class",
    "http-client"
  ],
  "peerDependencies": {
    "axios": "^1.13.2"
  },
  "devDependencies": {
    "@types/node": "^22.7.4",
    "tsup": "^8.0.2",
    "typescript": "^5.5.4"
  },
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm,cjs --target node18",
    "clean": "rm -rf dist"
  }
}
