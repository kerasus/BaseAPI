{
  "name": "api-basekit",
  "version": "1.0.3",
  "description": "Abstract base class for Axios-powered HTTP services with pagination helpers.",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "author": "a.esmaeeli",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kerasus/BaseAPI.git"
  },
  "bugs": {
    "url": "https://github.com/kerasus/BaseAPI/issues"
  },
  "homepage": "https://github.com/kerasus/BaseAPI#readme",
  "keywords": [
    "api-basekit",
    "axios",
    "typescript",
    "api",
    "pagination",
    "abstract-class",
    "http-client"
  ],
  "peerDependencies": {
    "axios": "^1.13.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^22.7.4",
    "tsup": "^8.0.2",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "eslint": "^9.30.1",
    "eslint-plugin-vue": "^10.3.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.5",
    "typescript": "^5.5.4",
    "vite": "^7.3.0",
    "vite-plugin-dts": "^4.5.4"
  },
  "scripts": {
    "lint": "eslint 'src/**/*.{ts,vue}'",
    "lint:fix": "eslint 'src/**/*.{ts,vue}' --fix",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "vite build",
    "build": "tsup src/index.ts --dts --format esm,cjs --target node18",
    "clean": "rm -rf dist"
  }
}
